{
  "plugins": ["@docusaurus"],
  "extends": ["plugin:@docusaurus/recommended", "prettier"],
  "rules": {
    "@docusaurus/string-literal-i18n-messages": "error",
    "@docusaurus/no-untranslated-text": "warn"
  },
  "ignorePatterns": ["**/node_modules/**", "**/.docusaurus/**", "docusaurus.config.js"]
}
